            <basicHttpBinding>
                <binding name="IBSServicesSoap" />
                <binding name="banksSoap" />
                <binding name="ServiceSoap" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://10.0.0.92:8090/SalesManagerWS/service.asmx"
                binding="basicHttpBinding" bindingConfiguration="ServiceSoap"
                contract="imalServ.ServiceSoap" name="ServiceSoap" />
            <endpoint address="http://localhost:814/ibsservices.asmx" binding="basicHttpBinding"
                bindingConfiguration="IBSServicesSoap" contract="ibs.IBSServicesSoap"
                name="IBSServicesSoap" />
            <endpoint address="http://localhost:824/banks.asmx" binding="basicHttpBinding"
                bindingConfiguration="banksSoap" contract="EACBS.banksSoap"
                name="banksSoap" />
        </client>
    </system.serviceModel>
