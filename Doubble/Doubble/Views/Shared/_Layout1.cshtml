<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- PAGE settings -->
    <link rel="icon" href="">
    <title>Doubble - Customer Login</title>
    <meta name="description" content="Doubble Sign up form by Sterling Bank Plc">
    <meta name="keywords" content="Doubble, sterling, sterling bank, savings">
    <meta name="author" content="Sterling Bank Plc">
    <!-- CSS dependencies -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" type="text/css">
    <link rel="stylesheet" href="~/wireframe.css"> 
</head>
<body>
    <header>
    <nav class="navbar navbar-expand-md navbar-dark bg-black">
        <div class="container-fluid">
            <a href="https://doubble.ng/#/main" title="Doubble" class="navbar-brand text-light">
                <img src="http://10.0.0.229/doubble/doubble_logo.png" alt="Doubble" style="display:block;border:0;outline:none;text-decoration:none;-ms-interpolation-mode:bicubic;" width="141">
            </a>

            <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarSupportedContent">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent"> </div>
        </div>
    </nav>
    </header>

    <div>
        @RenderBody()

        <footer id="footer">
            <div class="text-muted text-center py-2">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12 m-0">
                            <p class="mb-1">© 2018 Doubble by Sterling</p>
                        </div>
                    </div>
                </div>
            </div>
            </footer>
        </div>
            <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
            <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>