@model System.Data.DataTable
@using System.Data;

@{
    //ViewBag.Title = "Doubble - Let's get to know you.";
    Layout = "~/Views/Shared/_Layout1.cshtml";
}




<div class="col-md-3 left_col menu_fixed">
    <div class="left_col scroll-view">
        <div class="navbar nav_title" style="border: 0;">
            <a href="index.html" class="site_title"><i class="fas fa-adjust"></i> <span>Sterling Doubble</span></a>
        </div>
        <div class="clearfix"></div>
        <!-- menu profile quick info -->
        <div class="profile clearfix">
            <div class="fas fa-sign-out-alt" style="text-align: right;">@Html.ActionLink("Log Out", "LogOff", "Login")</div>

            <div class="profile_pic">
                <!-- Replace this with Name Abbreviation -->
                <div class="profile_icon">OO</div>
            </div>
            <div class="profile_info">
                <br>
                <h2>Welcome, @ViewBag.uname</h2>
            </div>
        </div>
        <!-- /menu profile quick info -->
        <br />
        <!-- sidebar menu -->
        <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
            <div class="menu_section">
                <h3>Menu</h3>
                <ul class="nav side-menu">
                    <li>@Html.ActionLink("Home", "DoubbleAdmin", "DoubbleAdmin")</li>
                    <li>@Html.ActionLink("New Accounts", "NewAccount", "DoubbleAdmin")</li>
                    <li>@Html.ActionLink("Future Date", "FutureDate", "DoubbleAdmin")</li>
                    <li>@Html.ActionLink("Diaspora", "Diaspora", "DoubbleAdmin")</li>
                    <li>@Html.ActionLink("No Subscription", "doubbleNoSub", "DoubbleAdmin")</li>
                    <li>@Html.ActionLink("New Account No Sub", "NewAccountNoSub", "DoubbleAdmin")</li>

                </ul>
            </div>
        </div>
        <!-- /sidebar menu -->
        <!-- /menu footer buttons -->
        <div class="sidebar-footer hidden-small">
            <a data-toggle="tooltip" data-placement="top" title="Home" href="index.html">
                <span class="glyphicon glyphicon-home" aria-hidden="true"></span>
            </a>
            <a data-toggle="tooltip" data-placement="top" title="Analytics" href="#">
                <span class="glyphicon glyphicon-dashboard" aria-hidden="true"></span>
            </a>
            <a data-toggle="tooltip" data-placement="top" title="Profile" href="profile.html">
                <span class="glyphicon glyphicon-cog" aria-hidden="true"></span>
            </a>
            <a data-toggle="tooltip" data-placement="top" title="Logout" href="login.html">
                <span class="glyphicon glyphicon-off" aria-hidden="true"></span>
            </a>
        </div>
        <!-- /menu footer buttons -->
    </div>
</div>
<!-- top navigation -->
<div class="top_nav">
    <div class="nav_menu">
        <nav>
            <div class="nav toggle">
                <a id="menu_toggle"><i class="fa fa-bars"></i></a>
            </div>
            <ul class="nav navbar-nav navbar-right">
                <li class="">
                    <a href="javascript:;" class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                        @ViewBag.fname
                        <span class=" fa fa-angle-down"></span>
                    </a>
                    <ul class="dropdown-menu dropdown-usermenu pull-right">
                        <li><a href="javascript:;"><i class="fas fa-cog"></i> Profile</a></li>

                        <li><div class="fas fa-sign-out-alt" style="text-align: right;">@Html.ActionLink("Log Out", "LogOff", "Login")</div></li>
                    </ul>
                </li>
            </ul>
        </nav>
    </div>
</div>
<!-- /top navigation -->
<!-- page content -->
<div class="right_col" role="main">
    <div class="">
        <div class="page-title">
            <div class="title_left">
                <h3>Dashboard <small> Sterling <strong>Admins</strong></small></h3>
            </div>
            <div class="title_right">
                <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
                </div>
            </div>
        </div>
        <div class="clearfix"></div>

        <div class="row">
            <div class="col-md-9 col-sm-9 col-xs-12">
                <div class="x_panel">
                    <div class="x_title">
                        <h2>Recent 100 Reports</h2>
                        <ul class="nav navbar-right panel_toolbox">
                            <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a></li>
                            <li>
                                <a class="close-link"><i class="fa fa-close"></i></a>
                            </li>
                        </ul>
                        <div class="clearfix"></div>
                    </div>
                    <div class="x_content">
@if (@ViewBag.Response != "No records to display")
{
                        <table id="datatable-buttons" class="table table-striped table-bordered">
                            <thead>
                                <tr>
                                    <th>First Name</th>
                                    <th>Last Name</th>
                                    <th>Email</th>
                                    <th>MobileNumber</th>
                                    <th>BVN</th>
                                    <th>DOB</th>
                                    <th>Entry Date</th>
                                    <th>Customer Status</th>
                                </tr>
                            </thead>

                            <tbody>

                                @foreach (DataRow row in Model.Rows)
                                {
                                    <tr>
                                        @foreach (DataColumn col in Model.Columns)
                                        {
                                            <td>@row[col.ColumnName]</td>
                                        }
                                    </tr>
                                }


                            </tbody>
                        </table>
                        using (Html.BeginForm("ExportdoubbleNoSub", "DoubbleAdmin", FormMethod.Post))
                        {
                            <input type="submit" value="Download as Excel" />
                        }
}
@if (@ViewBag.Response == "No records to display")
{
    @ViewBag.Response
}


                    </div>
                </div>
            </div>

        </div>

    </div>
</div>
<!-- /page content -->



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    <script type="text/javascript">




    </script>



}






















