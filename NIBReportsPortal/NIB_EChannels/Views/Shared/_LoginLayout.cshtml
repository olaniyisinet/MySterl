<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>@ViewBag.Title - NIB Channels Report</title>
	@Styles.Render("~/Content/css")
	@Scripts.Render("~/bundles/modernizr")

</head>
<body background="~/Content/images/bg.jpg"; background-repeat:no-repeat; background-size:auto">


	<div class=" body-content" style="margin: 5% 25%">
		@RenderBody()
		<hr />
		<footer style="text-align: center">
			<hr />
			<p>&copy; @DateTime.Now.Year - NIB Report Portal</p>
		</footer>
	</div>

	@Scripts.Render("~/bundles/jquery")
	@Scripts.Render("~/bundles/bootstrap")
	@RenderSection("scripts", required: false)

</body>
</html>
